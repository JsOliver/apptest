<?xml version='1.0' encoding='utf-8'?>
<widget id="webcarrao" version="1.0.11" xmlns="https://ncbrasil.com.br/" xmlns:cdv="https://ncbrasil.com.br/">
    <name>Webcarr√£o</name>
    <description>
        O maior portal de automoveis do Brasil.
    </description>
    <author email="joaovictordada2@gmail.com" href="https://ncbrasil.com.br/">
        NCBrasil
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
		
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
	    <icon src="icon.png" width="57" height="57" density="mdpi" />
		    <icon src="img/Icon.png" platform="ios" width="57" height="57" density="mdpi" />
		    <icon src="img/Icon-60.png" platform="ios" width="60" height="60" density="mdpi" />
		    <icon src="img/Icon-60@2x.png" platform="ios" width="60" height="60" density="mdpi" />
		    <icon src="img/Icon-60@3x.png" platform="ios" width="60" height="60" density="mdpi" />
		    <icon src="img/Icon-72.png" platform="ios" width="72" height="72" density="mdpi" />
		    <icon src="img/Icon-72@2x.png" platform="ios" width="72" height="72" density="mdpi" />
		    <icon src="img/Icon-76.png" platform="ios" width="76" height="76" density="mdpi" />
		    <icon src="img/Icon-76@2x.png" platform="ios" width="76" height="76" density="mdpi" />
		    <icon src="img/Icon-167.png" platform="ios" width="167" height="167" density="mdpi" />
		    <icon src="img/Icon-Small.png" platform="ios" width="29" height="29" density="mdpi" />
		    <icon src="img/Icon-Small-50.png" platform="ios" width="50" height="50" density="mdpi" />
		    <icon src="img/Icon-Small-50@2x.png" platform="ios" width="50" height="50" density="mdpi" />
		    <icon src="img/Icon-120.png" platform="ios" width="120" height="120" density="mdpi" />
